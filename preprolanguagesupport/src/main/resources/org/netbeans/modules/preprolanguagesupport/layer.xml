<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-prepro">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.preprolanguagesupport.Bundle"/>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="coloring.xml" url="prepro/fontColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.preprolanguagesupport.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>                
            </folder>
        </folder>
    </folder>
    
    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="prepro" url="prepro/ColorPreview.prepro"/>
            </folder>
        </folder>
    </folder>
</filesystem>

